<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/mainPage.css">
    <link rel="stylesheet" href="/css/join.css">
    <link rel="stylesheet" href="/bootstrapCSS_v5/bootstrap.css">
    <link rel="stylesheet" href="/bootstrapCSS_v5/bootstrap.min.css">
    <link rel="shortcut icon" type="image/x-icon" href="/images/icons8-system-task-16.png">
    <script src="https://kit.fontawesome.com/edff7358ca.js" crossorigin="anonymous"></script>
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous"> -->
    <title>Puget Sound Customs</title>
</head>
<body>
{{>parts/header}}

<!-- Join form  -->
<section class="h-[600px] w-[100%]">
    <div class="con">
        <div class="title_register">REGISTER</div>
        <div class="input_wrap">
            <div class="join_form">
                <label class="username block" for="username">UserName</label>
                <input id="join_username" type="text" name="username" placeholder="Username">

                <div class="password_wrap">
                    <div class="pw_wrap">
                        <label class="password block" for="password">Password</label>
                        <input id="join_password" type="text" name="password" placeholder="Password">
                    </div>
                    <div class="conf_pw_wrap">
                        <label class="confirm_password block" for="confirm_password">Confirm Password</label>
                        <input type="text" name="confirm_password" placeholder="confirm password">
                    </div>
                </div>
                <div class="email_wrap">
                    <div class="em_wrap">
                        <label class="email block" for="email">Email Address</label>
                        <input id="join_email" type="email" name="email" placeholder="email">
                    </div>
                    <div class="conf_em_wrap">
                        <label class="conf_email block" for="confirm_email">Email Address</label>
                        <input type="email" name="confirm_email" placeholder="confirm email">
                    </div>
                </div>

                <div class="btn_wrap flex">
                    <button class="join_create_account block">CREATE ACCOUNT</button>
                    <a href="/oauth2/authorization/google">Create account Through google?</a>
                </div>

            </div>
        </div>

    </div>

</section>
<!-- Join form  -->

{{>parts/header}}

<script src="/js/mainPage.js"></script>
<script src="/js/loginPage.js"></script>
<script src="/bootstrapJS_v5/bootstrap.bundle.js"></script>
<script src="/bootstrapJS_v5/bootstrap.bundle.min.js"></script>
</body>
</html>